{
  "name": "todo-next-v2",
  "version": "3.0.0",
  "description": "A boilerplate for Reactjs app using nextjs, redux, redux-thunk, antd, less",
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "antd",
    "less"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "MODE=development node bin/dev",
    "export": "MODE=production node bin/export",
    "build": "MODE=production node bin/build",
    "build:staging": "MODE=staging node bin/build",
    "start": "MODE=production node bin/start",
    "start:staging": "MODE=staging node bin/start",
    "lint": "next lint",
    "analyze": "MODE=production ANALYZE=true node bin/build",
    "serve": "serve .next/analyze"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "antd": "^4.16.7",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.6",
    "frontmatter-markdown-loader": "^3.6.3",
    "lodash": "^4.17.21",
    "next": "11.0.1",
    "next-redux-wrapper": "^7.0.2",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "rc-picker": "^2.5.14",
    "react": "17.0.2",
    "react-cookies": "^0.1.1",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-use": "^17.2.4",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less-vars-to-js": "^1.3.0",
    "next-plugin-antd-less": "^1.3.0",
    "webpack": "^5.45.1"
  }
}
